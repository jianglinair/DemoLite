<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lin.jiang.app"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:configChanges="keyboardHidden|orientation"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lin.jiang.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.lin.jiang.app.seek.SeekImageActivity">
        </activity>
        <activity android:name="com.lin.jiang.app.constraints.ConstraintsLayoutActivity">
        </activity>
        <activity android:name="com.lin.jiang.app.danmaku.DanmakuActivity"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>